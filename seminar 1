program 1

#include <iostream>
using namespace std;
int cmdc(int a, int b) {
    return b == 0 ? a : cmdc(b, a % b);
}
int main()
{
    int numar1, numar2;
    cin >> numar1 >> numar2;
    cout << cmdc(numar1, numar2) << endl;
    return 0;
}



program 2

#include <iostream>
using namespace std;
int main()
{
    int n,x, suma = 0;
    cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> x;
        suma += x;
    }
    cout << "Suma: " << suma << "\nMedia: " << (double)suma / n << endl;
    return 0;
}



program 3

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    double a, b, c, delta;
    cin >> a >> b >> c;
    delta = b * b - 4 * a * c;
    if (delta > 0)
    cout << (-b + sqrt(delta)) / (2 * a) << " " << (-b - sqrt(delta)) / (2 * a) << endl;
    else if (delta == 0)
    cout << -b / (2 * a) << endl;
    else
    cout << -b / (2 * a) << " + " << sqrt(-delta) / (2 * a) << "i "
    << -b / (2 * a) << " - " << sqrt(-delta) / (2 * a) << "i " << endl;
    return 0;
}
